{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "description": "Basic monitoring dashboard - Works with existing schema (before Week 1 migration)",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "datasource": {
        "type": "bigquery",
        "uid": "${datasource}"
      },
      "description": "Percentage of successful processing runs (completed + skipped)",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 95
              },
              {
                "color": "green",
                "value": 98
              }
            ]
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": {
            "type": "bigquery",
            "uid": "${datasource}"
          },
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT ROUND(COUNTIF(success = TRUE) / COUNT(*) * 100, 1) as value FROM `${project_id}.nba_processing.analytics_processor_runs` WHERE run_date >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 24 HOUR)",
          "refId": "A"
        }
      ],
      "title": "Success Rate (24h)",
      "type": "gauge"
    },
    {
      "datasource": {
        "type": "bigquery",
        "uid": "${datasource}"
      },
      "description": "Average processing duration for completed runs",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 30
              },
              {
                "color": "red",
                "value": 60
              }
            ]
          },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 2,
      "options": {
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": {
            "type": "bigquery",
            "uid": "${datasource}"
          },
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT ROUND(AVG(duration_seconds), 1) as value FROM `${project_id}.nba_processing.analytics_processor_runs` WHERE run_date >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 24 HOUR) AND success = TRUE",
          "refId": "A"
        }
      ],
      "title": "Avg Duration (24h)",
      "type": "gauge"
    },
    {
      "datasource": {
        "type": "bigquery",
        "uid": "${datasource}"
      },
      "description": "Processing duration trend by processor over last 7 days",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "Duration (seconds)",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 20,
            "gradientMode": "none",
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            },
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 6
      },
      "id": 3,
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "bigquery",
            "uid": "${datasource}"
          },
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT TIMESTAMP_TRUNC(run_date, HOUR) as time, processor_name as metric, AVG(duration_seconds) as value FROM `${project_id}.nba_processing.analytics_processor_runs` WHERE run_date >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 7 DAY) AND success = TRUE AND processor_name LIKE CONCAT('%', '${processor:raw}', '%') GROUP BY time, metric ORDER BY time",
          "refId": "A"
        }
      ],
      "title": "Duration Over Time (7d)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "bigquery",
        "uid": "${datasource}"
      },
      "description": "Recent processing runs with basic details",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "left",
            "cellOptions": {
              "type": "auto"
            },
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "success"
            },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-background"
                }
              },
              {
                "id": "mappings",
                "value": [
                  {
                    "options": {
                      "true": {
                        "color": "green",
                        "index": 0,
                        "text": "Success"
                      },
                      "false": {
                        "color": "red",
                        "index": 1,
                        "text": "Failed"
                      }
                    },
                    "type": "value"
                  }
                ]
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 14
      },
      "id": 4,
      "options": {
        "cellHeight": "sm",
        "footer": {
          "countRows": false,
          "fields": "",
          "reducer": ["sum"],
          "show": false
        },
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "run_date"
          }
        ]
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": {
            "type": "bigquery",
            "uid": "${datasource}"
          },
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT run_date, processor_name, date_range_start, date_range_end, success, ROUND(duration_seconds, 1) as duration_seconds, records_processed FROM `${project_id}.nba_processing.analytics_processor_runs` WHERE run_date >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 24 HOUR) AND processor_name LIKE CONCAT('%', '${processor:raw}', '%') ORDER BY run_date DESC LIMIT 100",
          "refId": "A"
        }
      ],
      "title": "Recent Executions (24h)",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {},
            "indexByName": {
              "run_date": 0,
              "processor_name": 1,
              "date_range_start": 2,
              "date_range_end": 3,
              "success": 4,
              "duration_seconds": 5,
              "records_processed": 6
            },
            "renameByName": {
              "run_date": "Time",
              "processor_name": "Processor",
              "date_range_start": "Start Date",
              "date_range_end": "End Date",
              "success": "Status",
              "duration_seconds": "Duration (s)",
              "records_processed": "Records"
            }
          }
        }
      ],
      "type": "table"
    }
  ],
  "refresh": "1m",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["nba-props", "phase1", "basic-monitoring"],
  "templating": {
