          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH weekly_metrics AS ( SELECT processor_name, ROUND(AVG(waste_pct), 1) as avg_waste_pct, COUNT(*) as total_runs, SUM(duration_seconds * waste_pct / 100) / 3600 as wasted_hours_per_week FROM `${project_id}.nba_processing.analytics_processor_runs` WHERE run_date >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 7 DAY) AND success = TRUE AND waste_pct IS NOT NULL GROUP BY processor_name ), decision AS ( SELECT processor_name, avg_waste_pct, total_runs, ROUND(wasted_hours_per_week, 2) as wasted_hours_per_week, CASE WHEN avg_waste_pct > 30 AND wasted_hours_per_week > 2 AND total_runs > 10 THEN ROUND(3 / wasted_hours_per_week, 1) ELSE NULL END as weeks_to_roi, CASE WHEN avg_waste_pct > 30 AND wasted_hours_per_week > 2 AND total_runs > 10 AND (3 / wasted_hours_per_week) < 8 THEN 'ðŸ”´ IMPLEMENT PHASE 3' WHEN avg_waste_pct > 20 OR wasted_hours_per_week > 1 THEN 'ðŸŸ¡ MONITOR CLOSELY' ELSE 'ðŸŸ¢ SKIP - Phase 1 Sufficient' END as recommendation FROM weekly_metrics ) SELECT processor_name, avg_waste_pct, wasted_hours_per_week, total_runs, weeks_to_roi, recommendation FROM decision WHERE processor_name LIKE CONCAT('%', '${processor:raw}', '%') ORDER BY avg_waste_pct DESC",
