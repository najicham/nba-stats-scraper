# File: backfill_jobs/raw/odds_game_lines/job-config.env
# Description: Cloud Run job configuration for Odds API game lines processor backfill

# CRITICAL: Use unique name - add "processor" to distinguish from scraper jobs
JOB_NAME="odds-game-lines-processor-backfill"
JOB_SCRIPT="backfill_jobs/raw/odds_game_lines/odds_game_lines_raw_backfill.py"
JOB_DESCRIPTION="Process Odds API game lines history data from GCS to BigQuery"

# Resources - odds data has many nested records per file
TASK_TIMEOUT="21600"  # 6 hours (should be enough)
MEMORY="4Gi"
CPU="2"

# Defaults
START_DATE="2021-10-01"
END_DATE="2025-06-30"
BUCKET_NAME="nba-scraped-data"