# backfill_jobs/raw/bdl_boxscores/job-config.env
# CRITICAL: Use unique name - add "processor" to distinguish from scraper jobs
JOB_NAME="bdl-boxscores-processor-backfill"
JOB_SCRIPT="backfill_jobs/raw/bdl_boxscores/bdl_boxscores_raw_backfill.py"
JOB_DESCRIPTION="Process Ball Don't Lie box scores from GCS to BigQuery"

# Resources - Box scores processing requires moderate resources
TASK_TIMEOUT="7200"  # 2 hours (games have ~30-35 players each)
MEMORY="4Gi"
CPU="2"
MAX_RETRIES="1"

# Default parameters
START_DATE="2021-10-01"
END_DATE="2025-06-30"
BUCKET_NAME="nba-scraped-data"

# Environment variables for the job
ENV_VARS="GCP_PROJECT_ID=nba-props-platform,BUCKET_NAME=nba-scraped-data"
