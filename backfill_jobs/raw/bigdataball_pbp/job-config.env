# File: backfill_jobs/raw/bigdataball_pbp/job-config.env
# Description: Cloud Run job configuration for BigDataBall play-by-play processor backfill

# CRITICAL: Use unique name - add "processor" to distinguish from scraper jobs
JOB_NAME="bigdataball-pbp-processor-backfill"
JOB_SCRIPT="backfill_jobs/raw/bigdataball_pbp/bigdataball_pbp_raw_backfill.py"
JOB_DESCRIPTION="Process BigDataBall play-by-play data from GCS to BigQuery"

# Resources (BigDataBall files can be large with 500-800 events per game)
TASK_TIMEOUT="7200"  # 1 hour
MEMORY="4Gi"         # 4GB for processing large JSON files
CPU="2"              # 2 cores for parallel processing

# Defaults (typical NBA season range)
START_DATE="2024-10-01"  # NBA season typically starts in October
END_DATE="2025-06-30"    # NBA season ends in June
BUCKET_NAME="nba-scraped-data"