# File: backfill_jobs/raw/nbac_schedule/job-config.env
# Description: Cloud Run job configuration for NBA.com schedule processor backfill

# CRITICAL: Use unique name - add "processor" to distinguish from scraper jobs
JOB_NAME="nbac-schedule-processor-backfill"
JOB_SCRIPT="backfill_jobs/raw/nbac_schedule/nbac_schedule_raw_backfill.py"
JOB_DESCRIPTION="Process NBA.com schedule data from GCS to BigQuery"

# Resources
TASK_TIMEOUT="1800"  # 30 minutes (should be plenty for 4 seasons)
MEMORY="4Gi"
CPU="2"

# Defaults
SEASON=""  # Will process all seasons if not specified
BUCKET_NAME="nba-scraped-data"