# File: backfill_jobs/raw/nbac_player_boxscore/job-config.env
# Description: Cloud Run job configuration for NBA.com player boxscore processor backfill

# CRITICAL: Use unique name - add "processor" to distinguish from scraper jobs
JOB_NAME="nbac-player-boxscore-processor-backfill"
JOB_SCRIPT="backfill_jobs/raw/nbac_player_boxscore/nbac_player_boxscore_raw_backfill.py"
JOB_DESCRIPTION="Process NBA.com player boxscore data from GCS to BigQuery"

# Resources (enhanced for NBA.com data processing)
TASK_TIMEOUT="7200"  # 2 hours for full season processing
MEMORY="4Gi"         # 4GB for large JSON processing
CPU="2"              # 2 CPUs for parallel processing

# Default parameters
START_DATE="2021-10-01"  # Start of 2021-22 NBA season
END_DATE="2025-06-30"    # End of 2024-25 NBA season  
BUCKET_NAME="nba-scraped-data"

# Supported seasons for --seasons parameter
AVAILABLE_SEASONS="2021,2022,2023,2024"  # 2021-22 through 2024-25 seasons